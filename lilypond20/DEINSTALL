#!@SH@

PREFIX="@PREFIX@"
PKG_PREFIX="@PREFIX@"
LOCALBASE="@LOCALBASE@"

PKGBASE="@PKGBASE@"
PKGVERSION="@PKGVERSION@"

case $2 in
        DEINSTALL)
                datadir=${PKG_PREFIX}/share/${PKGBASE}/${PKGVERSION}/
                rm -f ${datadir}/ls-R
		rm -f ${datadir}/python/*.pyo
		rm -f ${datadir}/python/*.pyc
        ;;
esac
